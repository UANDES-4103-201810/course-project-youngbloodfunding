<p id="notice"><%= notice %></p>

<h1>Promises</h1>

<table>
  <thead>
    <tr>
      <th>Project id</th>
      <th>Price</th>
      <th>Description</th>
      <th>Delivery date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @promises.each do |promise| %>
      <tr>
        <td><%= promise.project.id %></td>
        <td><%= promise.price %></td>
        <td><%= promise.description %></td>
        <td><%= promise.estimated_delivery %></td>
        <td><%= link_to 'Show', promise %></td>
        <td><%= link_to 'Edit', edit_promise_path(promise) %></td>
        <td><%= link_to 'Destroy', promise, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

