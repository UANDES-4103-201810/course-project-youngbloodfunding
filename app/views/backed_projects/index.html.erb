<p id="notice"><%= notice %></p>

<h1>Backed Projects</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @backed_projects.each do |backed_project| %>
      <tr>
        <td><%= link_to 'Show', backed_project %></td>
        <td><%= link_to 'Edit', edit_backed_project_path(backed_project) %></td>
        <td><%= link_to 'Destroy', backed_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Backed Project', new_backed_project_path %>
